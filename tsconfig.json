{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["assets/*"],
      "@styles/*": ["src/styles/*"],
      "@components/*": ["src/components/*"],
      "@context/*": ["src/context/*"],
      "@types/*": ["src/types/*"]
    },
    "ignoreDeprecations": "6.0"
  }
}
